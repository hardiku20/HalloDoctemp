<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - learning1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/learning1.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://getbootstrap.com/docs/5.3/assets/css/docs.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true">
    

</head>
<body class="bg-light">
    <!-- Navbar of the Page -->
    <header class="sticky-top">
        <div class="d-md-block d-none shadow-sm bg-white">
            <nav class="Navbar d-flex justify-content-between align-items-center ">
                <div class="ms-3">
                    <a href=""><img src="/assets/images/logo.png" alt="" class="img-thumbnail border-0 w-50 h-50"></a>
                </div>
                <div class="d-flex align-items-center">
                    <span class="me-3">Welcome <strong>Tests Testing</strong></span>
                    <div class="me-3">
                        <a href="patientsite.html"
                           class="btn btn-outline-info font-info text-decoration-none text-info fs-5 p-1 bg-white">Logout</a>
                    </div>
                    <div class="me-3">
                        <button type="submit" class="rounded border-info bg-white border ">
                            <img src="/assets/images/moon.svg"
                                 style="transform: rotate(90deg);" alt="" class="p-1">
                        </button>
                    </div>
                </div>


            </nav>
            <hr>
            <div class="d-flex align-items-center">



                <ul class="nav nav-underline mx-3">

                    <li class="nav-item">
                        <a class="nav-link active text-info px-2 fw-medium" aria-current="page"
                           asp-action="admindashboard" asp-controller="Admin">DashBoard</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary px-2 fw-medium" href="#">Provider Location</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary px-2 fw-medium" asp-action="AdminProfile" asp-controller="Admin">My Profile</a>
                    </li>
                    <li class="nav-item dropdown">
                        @* <a class="nav-link text-secondary px-2 fw-medium" href="#">Providers</a> *@
                            @* <button class="nav-link btn dropdown-toggle text-secondary fw-medium px-2 rounded-0" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Provider
                            </button>
                            <ul class="dropdown-menu border-0 rounded-0 shadow-lg">
                                <li><a class="dropdown-item" >Provider</a></li>
                                <li><a class="dropdown-item" href="#">Scheduling</a></li>
                                <li><a class="dropdown-item" href="#">Invoicing</a></li>
                            </ul>
 *@

                        <div class="dropdown">
                            <a role="button" class="nav-link text-secondary px-2 fw-medium" data-bs-toggle="dropdown">Providers</a>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                                <li><a role="button" class="dropdown-item" asp-action="ProviderMenu" asp-controller="Admin">Provider</a></li>
                                <li><a role="button" class="dropdown-item" >Scheduling</a></li>
                                <li><a role="button" class="dropdown-item">Invoicing</a></li>
                            </ul>
                        </div>
                        
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary px-2 fw-medium" href="#">Partners</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary px-2 fw-medium" href="#">Access</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary px-2 fw-medium" href="#">Records</a>
                    </li>
                </ul>

            </div>

        </div>


        <div class="d-md-none shadow bg-white py-2">
            <nav class="Navbar d-flex justify-content-between align-items-center mx-4">
                <div class="">
                    <button class="btn btn-outline-secondary border-0" type="button" data-bs-toggle="offcanvas"
                            data-bs-target="#staticBackdrop" aria-controls="staticBackdrop">
                        <img src="/assets/images/list.svg" alt="">
                    </button>
                    <a href=""><img src="/assets/images/logo.png" alt="" class="img-thumbnail border-0 w-50 h-50"></a>
                </div>
                <div class="d-flex">
                    <div class="me-2">
                        <button type="submit" class="rounded border-info bg-white border">
                            <img src="/assets/images/box-arrow-right.svg" class="p-1" alt="">
                        </button>
                    </div>
                    <div class="">
                        <button type="submit" class="rounded border-info bg-white border ">
                            <img src="/assets/images/moon.svg"
                                 style="transform: rotate(90deg);" alt="" class="p-1">
                        </button>
                    </div>
                </div>
            </nav>
        </div>

        <div class="offcanvas offcanvas-start" data-bs-backdrop="static" tabindex="-1" id="staticBackdrop"
             aria-labelledby="staticBackdropLabel">
            <div class="offcanvas-header d-flex justify-content-end">
                <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
            </div>
            <div class="offcanvas-body p-0">
                <ul class="nav d-block">

                    <li class="nav-item bg-danger-subtle">
                        <a class="nav-link active text-danger" aria-current="page"
                           href="PatientDashboard.html">DashBoard</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary" href="#">Provider Location</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary" href="#">My Profile</a>
                    </li>
                    <li class="nav-item dropdown">
                       @*  <a class="nav-link text-secondary" href="#">Providers</a> *@
                        <div class="dropdown">
                            <a role="button" class="nav-link text-secondary px-2 fw-medium" data-bs-toggle="dropdown">Providers</a>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                                <li><a role="button" class="dropdown-item" asp-action="ProviderMenu" asp-controller="Admin">Provider</a></li>
                                <li><a role="button" class="dropdown-item">Scheduling</a></li>
                                <li><a role="button" class="dropdown-item">Invoicing</a></li>
                            </ul>
                        </div>
                     
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary" href="#">Partners</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary" href="#">Access</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-secondary" href="#">Records</a>
                    </li>
                </ul>

            </div>
        </div>





    </header>




    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @*  <footer class="border-top footer text-muted">
    <div class="container">
    &copy; 2024 - learning1 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
    </footer> *@
 @*    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script> *@
    @* @await RenderSectionAsync("Scripts", required: false) *@
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await Component.InvokeAsync("Notyf")
    @RenderSection("Scripts", required: false)
</body>
</html>
