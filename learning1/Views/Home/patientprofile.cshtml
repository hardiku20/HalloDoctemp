@model learning1.DBEntities.ViewModel.PatientProfileViewModel;@*    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860*@@{    Layout = null;}<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0">    <title>Patient DashBoard</title>    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">    <link rel="stylesheet" href="css/style.css"></head><body class="bg-light">    <div class="modal fade" id="MapModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">        <div class="modal-dialog modal-dialog-centered">            <div class="modal-content">                <section>                    <div class="mapouter">                        <div class="gmap_canvas">                            <iframe width="600" height="500" id="gmap_canvas" src="" frameborder="0" scrolling="no" marginheight="0" marginwidth="0">                                <!-- </iframe><a href="https://123movies-to.org">123 movies</a><br> -->                                <style>                                    .mapouter {                                        position: relative;                                        text-align: right;                                        height: 500px;                                        width: 600px;                                    }</style>                                <!-- <a href="https://www.embedgooglemap.net">embedgooglemap.net</a> -->                                <style>                                    .gmap_canvas {                                        overflow: hidden;                                        background: none !important;                                        height: 500px;                                        width: 600px;                                    }                                </style>                        </div>                    </div>                </section>            </div>        </div>    </div>    <!-- Navbar of the Page -->    <header class="shadow bg-white">        <nav class="Navbar d-flex justify-content-between align-items-center ">            <div class="ms-3">                <a href=""><img src="/assets/images/logo.png" alt="" class="img-thumbnail border-0 w-50 h-50"></a>            </div>            <div class="d-flex align-items-center">                <span class="me-3">Welcome <strong>@Model.UserName</strong></span>                <div class="me-3">                    <a href="patientsite.html" class="btn btn-outline-info font-info text-decoration-none text-info fs-5 p-1 bg-white">Logout</a>                </div>                <div class="me-3">                    <button type="submit" class="rounded border-info bg-white border ">                        <img src="/assets/images/moon.svg" style="transform: rotate(90deg);" alt="" class="p-1">                    </button>                </div>            </div>        </nav>        <hr />        <div class="d-flex align-items-center">            <ul class="nav nav-underline ms-5">                <li class="nav-item">                    <a class="nav-link  text-secondary fw-normal px-2 fw-medium" asp-controller="home" asp-action="patient_dashboard">DashBoard</a>                </li>                <li class="nav-item">                    <a class="nav-link active text-info px-2 fw-medium" aria-current="page" asp-controller="home" asp-action="patient_profile">Profile</a>                </li>            </ul>        </div>    </header>    <section class="">        <div class="container pb-4">            <div class="d-flex justify-content-between align-items-center">                <h4>User Profile</h4>                <div class="d-flex justify-content-end m-4">                    <a asp-action="patientdashboard" asp-controller="home" class="btn btn-outline-info font-info text-decoration-none text-info fs-5 bg-white">                        <img src="/assets/images/chevron-left.svg" alt=""> Back                    </a>                </div>            </div>            <div class="card">                <div class="card-body">                    <form action="" class="form-floating" method="post">                        <div class="row">                            <h5 class="mt-3 mb-2">General Information</h5>                            <div class="mb-3 col-6">                                <div class="form-floating input-info">                                    <input type="text" class="form-control" id="floatingInput" placeholder="First Name" value=@Model.FirstName disabled asp-for="FirstName">                                    <label for="floatingInput">First Name</label>                                </div>                            </div>                            <div class="mb-3 col-6">                                <div class="form-floating input-info">                                    <input type="text" class="form-control" id="floatingInput" value=@Model.LastName placeholder="Last Name" disabled asp-for="LastName">                                    <label for="floatingInput">Last Name</label>                                </div>                            </div>                            <div class="form-floating col-6 mb-3 input-info">                                <input type="date" class="form-control" id="floatingInputValue" aria-label="Date" aria-describedby="basic-addon1" disabled asp-for="DateOfBirth" value=@Model.DateOfBirth>                                <label for="floatingInputValue" class="mx-2">Date of birth</label>                            </div>                        </div>                        <div class="row">                            <h5 class=" mt-3 mb-2">Contact Information</h5>                            <div class="col-6">                                <div class="row mt-2 justify-content-between">                                    <div class="form-floating input-info col-4">                                        <select class="form-select" id="floatingSelectDisabled" aria-label="Floating label disabled select example" disabled>                                            <option selected>Mobile</option>                                            <option value="1">One</option>                                            <option value="2">Two</option>                                            <option value="3">Three</option>                                        </select>                                        <label for="floatingSelectDisabled" class="mx-2">Type</label>                                    </div>                                    <div class="form-floating col-8">                                        <input type="number" class="form-control" id="floatingInput" placeholder="number" value=@Model.PhoneNo disabled asp-for="PhoneNo">                                        <label for="floatingInput" class="mx-2">Phone Number</label>                                    </div>                                </div>                            </div>                            <div class="col-md mt-2">                                <div class="form-floating input-info">                                    <input type="email" class="form-control" id="floatingInput" mailto:placeholder="name@example.com" value=@Model.Email disabled asp-for="Email">                                    <label for="floatingInput">Email address</label>                                </div>                            </div>                        </div>                        <div class="row">                            <h5 class="mt-3 mb-2">Location Information</h5>                            <div class="col-sm-6 mb-3 mt-2">                                <div class="form-floating input-info">                                    <input type="text" class="form-control" id="Street" placeholder="Street" value=@Model.Street disabled asp-for="Street">                                    <label for="floatingInput">Street</label>                                </div>                            </div>                            <div class="col-sm-6 mb-3 mt-2">                                <div class="form-floating input-info">                                    <input type="text" class="form-control" id="City" placeholder="City" value=@Model.City disabled asp-for="City">                                    <label for="floatingInput">City</label>                                </div>                            </div>                            <div class="col-sm-6 mb-3">                                <div class="form-floating input-info">                                    <input type="text" class="form-control" id="State" placeholder="State" value=@Model.State disabled asp-for="State">                                    <label for="floatingInput">State</label>                                </div>                            </div>                            <div class="col-sm-6 mb-3">                                <div class="row ">                                    <div class="form-floating input-info col-8">                                        <input type="text" class="form-control" id="ZipCode" placeholder="ZipCode" value=@Model.ZipCode disabled asp-for="ZipCode">                                        <label for="floatingInput" class="mx-2">Zip Code</label>                                    </div>                                    <div class="col-2">                                        <button type="button" class="btn btn-outline-info  text-decoration-none bg-white text-info   d-flex  align-items-center p-3" id="Map" data-bs-toggle="modal" data-bs-target="#MapModal">                                            <img src="~/assets/images/geo-alt-fill.svg" class="me-2" alt="...">Map                                        </button>                                    </div>                                </div>                            </div>                        </div>                        <div>                            <div class="d-flex mt-3 mb-2 justify-content-end">                                <button type="button" class="btn btn-info text-white text-decoration-none px-4 py-2" id="editButton" >                                    Edit                                </button>                            </div>                            <div class="d-flex savebutton justify-content-end d-none gap-3 me-2 mb-5">                                <button class="btn btn-info text-white" type="submit">Submit</button>                                <button class="btn border-info text-info" asp-action="PatientProfile">Cancel</button>                            </div>                        </div>                    </form>                </div>            </div>        </div>    </section>    <script>        document.getElementById("editButton").addEventListener("click", function () {            // Get all disabled input elements within the form            var disabledInputs = document.querySelectorAll("form input[disabled]");            // Iterate over each disabled input element and enable it            disabledInputs.forEach(function (input) {                input.removeAttribute("disabled");            });            // Enable select elements as well            var disabledSelects = document.querySelectorAll("form select[disabled]");            disabledSelects.forEach(function (select) {                select.removeAttribute("disabled");            });            var editbtn = document.getElementById("editButton").classList.add("d-none");            var savebtn = document.querySelector(".savebutton").classList.remove("d-none");            document.querySelector(".savebutton").classList.add("d-flex");        });    </script>    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>    <script>        debugger;        $(document).ready(function () {            $("#Map").click(function () {                var Street = $("#Street").val();                var City = $("#City").val();                var State = $("#State").val();                var ZipCode = $("#ZipCode").val();                // Define the address to geocode                var address = "https://maps.google.com/maps?q=" + Street + City + State + ZipCode + "&t=&z=13&ie=UTF8&iwloc=&output=embed";                $("#gmap_canvas").attr("src", address);            });        });    </script>    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>    <script src="https://code.jquery.com/jquery-3.6.3.min.js"></script></body></html>